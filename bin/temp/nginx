
server {
	# SSL configuration
	listen 9082 ssl default_server;
	# Fraunhofer Certificates:
	ssl_certificate /home/bgw/srv.pem;
	ssl_certificate_key /home/bgw/key.pem;

	root /var/www/html;

	# Add index.php to the list if you are using PHP
	index index.html index.htm index.nginx-debian.html;

	server_name _;

 	location / {
                proxy_pass         http://localhost:9080;
                rewrite  ^/hds/?(.*)  /$1 break;
                proxy_set_header   Host $host;
        }

}
